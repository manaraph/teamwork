{
  "name": "teamwork-backend",
  "version": "1.0.0",
  "description": "Teamwork is an internal social network for employees of an organization. The goal of this application is to facilitate more interaction between colleagues and promote team bonding.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node ./src/index.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "build": "babel src --out-dir ./dist --source-maps",
    "server": "node ./dist/index.js",
    "debug": "node --inspect-brk ./dist/index.js",
    "test": "./node_modules/.bin/mocha --compilers js:@babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manaraph/teamwork-backend.git"
  },
  "author": "Manasseh Omachonu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manaraph/teamwork-backend/issues"
  },
  "homepage": "https://github.com/manaraph/teamwork-backend#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "pg": "^7.12.1"
  }
}
